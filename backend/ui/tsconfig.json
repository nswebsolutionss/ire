{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "src/*": ["./src/*"],
      "@dwellio/components": ["./src/components/index.ts"],
      "@dwellio/pages": ["./src/pages/index.ts"],
      "assets/*":["./src/assets/*"]

    },
    "target": "ESNext",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext",
    ],
    "types": ["vite/client", "vite-plugin-svgr/client"],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "downlevelIteration": true
  },
  "include": [
    "src/"
  ]
}
